{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsx-boolean-value": [
      "error",
      "never"
    ],
    "jsx-no-multiline-js": [
      "error",
      "never"
    ],
    "member-access": [
      false
    ],
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-single"
    ]
  }
}